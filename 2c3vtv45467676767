-- â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

-- âš ï¸ DEINE GITHUB RAW URL HIER EINSETZEN:
local SpeedLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/DEINNAME/DEINREPO/main/SpeedLib.lua"))()

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--  Services & Shortcuts
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
local Players       = game:GetService("Players")
local RunService    = game:GetService("RunService")
local Lighting      = game:GetService("Lighting")
local UIS           = game:GetService("UserInputService")
local LocalPlayer   = Players.LocalPlayer

local function Char()  return LocalPlayer.Character end
local function Hum()   return Char() and Char():FindFirstChildOfClass("Humanoid") end
local function Root()  return Char() and Char():FindFirstChild("HumanoidRootPart") end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--  WINDOW
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
local Win = SpeedLib:CreateWindow({
    Title     = "âš¡ Speed Hub",
    Version   = "v2.0",
    Theme     = "Dark",                     -- "Dark" | "Neon" | "Blue"
    ToggleKey = Enum.KeyCode.RightShift,
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--  TAB 1 - PLAYER
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
local TabPlayer = Win:CreateTab("Player", "ğŸ‘¤")

-- Section: Movement
local Move = TabPlayer:CreateSection("Movement")

Move:AddSlider({
    Name = "Walk Speed",
    Min = 16, Max = 500, Default = 16,
    Callback = function(v)
        if Hum() then Hum().WalkSpeed = v end
    end
})

Move:AddSlider({
    Name = "Jump Power",
    Min = 7, Max = 300, Default = 7,
    Callback = function(v)
        if Hum() then Hum().JumpPower = v end
    end
})

Move:AddToggle({
    Name = "Infinite Jump",
    Default = false,
    Callback = function(v) _G.InfJump = v end
})
UIS.JumpRequest:Connect(function()
    if _G.InfJump and Hum() then
        Hum():ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

Move:AddToggle({
    Name = "No Clip",
    Default = false,
    Callback = function(v) _G.NoClip = v end
})
RunService.Stepped:Connect(function()
    if _G.NoClip and Char() then
        for _, p in pairs(Char():GetDescendants()) do
            if p:IsA("BasePart") then p.CanCollide = false end
        end
    end
end)

Move:AddButton({
    Name = "Reset Charakter",
    ButtonText = "Reset",
    Callback = function()
        if Hum() then Hum().Health = 0 end
        Win:Notify({ Title = "Reset", Message = "Charakter resettet.", Type = "Warning", Duration = 3 })
    end
})

-- Section: Combat
local Combat = TabPlayer:CreateSection("Combat")

Combat:AddToggle({
    Name = "God Mode",
    Default = false,
    Callback = function(v)
        if Hum() then
            Hum().MaxHealth = v and math.huge or 100
            Hum().Health    = v and math.huge or 100
        end
        Win:Notify({ Title = v and "God Mode AN" or "God Mode AUS", Message = v and "Unverwundbar!" or "Deaktiviert.", Type = v and "Success" or "Warning", Duration = 3 })
    end
})

Combat:AddToggle({
    Name = "Anti Knock",
    Default = false,
    Callback = function(v) _G.AntiKnock = v end
})
RunService.Heartbeat:Connect(function()
    if _G.AntiKnock and Root() then
        Root().AssemblyAngularVelocity = Vector3.zero
    end
end)

Combat:AddButton({
    Name = "Kill All",
    Description = "Alle Spieler tÃ¶ten",
    ButtonText = "Kill",
    Callback = function()
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer and p.Character then
                local h = p.Character:FindFirstChildOfClass("Humanoid")
                if h then h.Health = 0 end
            end
        end
        Win:Notify({ Title = "Kill All", Message = "Alle eliminiert.", Type = "Success", Duration = 3 })
    end
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--  TAB 2 - VISUAL
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
local TabVisual = Win:CreateTab("Visual", "ğŸ‘")

-- Section: ESP
local ESP = TabVisual:CreateSection("ESP")

ESP:AddToggle({
    Name = "Player ESP",
    Default = false,
    Callback = function(v)
        _G.ESP = v
        if not v then
            for _, p in pairs(Players:GetPlayers()) do
                if p ~= LocalPlayer and p.Character then
                    local hl = p.Character:FindFirstChild("ESP_HL")
                    if hl then hl:Destroy() end
                end
            end
        end
        Win:Notify({ Title = "ESP", Message = v and "ESP an." or "ESP aus.", Type = v and "Success" or "Warning", Duration = 2 })
    end
})

RunService.Heartbeat:Connect(function()
    if not _G.ESP then return end
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= LocalPlayer and p.Character and not p.Character:FindFirstChild("ESP_HL") then
            local hl = Instance.new("Highlight")
            hl.Name = "ESP_HL"
            hl.FillColor = _G.ESPColor or Color3.fromRGB(255, 50, 50)
            hl.OutlineColor = Color3.fromRGB(255, 255, 255)
            hl.FillTransparency = 0.5
            hl.Parent = p.Character
        end
    end
end)

ESP:AddDropdown({
    Name = "ESP Farbe",
    Options = { "Rot", "GrÃ¼n", "Blau", "Orange", "Gelb", "Lila" },
    Default = "Rot",
    Callback = function(v)
        local colors = {
            Rot    = Color3.fromRGB(255, 50, 50),
            GrÃ¼n   = Color3.fromRGB(50, 255, 100),
            Blau   = Color3.fromRGB(50, 100, 255),
            Orange = Color3.fromRGB(255, 140, 0),
            Gelb   = Color3.fromRGB(255, 255, 50),
            Lila   = Color3.fromRGB(180, 50, 255),
        }
        _G.ESPColor = colors[v]
        Win:Notify({ Title = "ESP Farbe", Message = v .. " ausgewÃ¤hlt.", Type = "Info", Duration = 2 })
    end
})

-- Section: World
local World = TabVisual:CreateSection("World")

World:AddToggle({
    Name = "Fullbright",
    Default = false,
    Callback = function(v)
        Lighting.Ambient        = v and Color3.fromRGB(255, 255, 255) or Color3.fromRGB(70, 70, 70)
        Lighting.OutdoorAmbient = v and Color3.fromRGB(255, 255, 255) or Color3.fromRGB(127, 127, 127)
        Lighting.Brightness     = v and 2 or 1
    end
})

World:AddToggle({
    Name = "No Fog",
    Default = false,
    Callback = function(v)
        Lighting.FogEnd = v and 999999 or 1000
    end
})

World:AddSlider({
    Name = "Tageszeit",
    Min = 0, Max = 24, Default = 14, Increment = 1,
    Callback = function(v)
        Lighting.TimeOfDay = string.format("%02d:00:00", v)
    end
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--  TAB 3 - TELEPORT
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
local TabTP = Win:CreateTab("Teleport", "ğŸŒ€")

local TP = TabTP:CreateSection("Teleport")

TP:AddButton({
    Name = "Zum Spawn",
    Description = "Teleportiert zu 0, 5, 0",
    ButtonText = "TP",
    Callback = function()
        if Root() then Root().CFrame = CFrame.new(0, 5, 0) end
        Win:Notify({ Title = "Teleport", Message = "Zum Spawn!", Type = "Success", Duration = 2 })
    end
})

TP:AddButton({
    Name = "ZufÃ¤llige Position",
    Description = "Irgendwo auf der Map",
    ButtonText = "TP",
    Callback = function()
        if Root() then
            Root().CFrame = CFrame.new(math.random(-500, 500), 50, math.random(-500, 500))
        end
        Win:Notify({ Title = "Random TP", Message = "Irgendwo gelandet!", Type = "Info", Duration = 2 })
    end
})

TP:AddButton({
    Name = "Nach oben",
    Description = "300 Studs nach oben",
    ButtonText = "TP",
    Callback = function()
        if Root() then Root().CFrame = Root().CFrame + Vector3.new(0, 300, 0) end
    end
})

local TPPlyr = TabTP:CreateSection("Zu Spieler")

local function GetPlayers()
    local t = {}
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= LocalPlayer then table.insert(t, p.Name) end
    end
    return #t > 0 and t or {"Kein Spieler"}
end

TPPlyr:AddDropdown({
    Name = "Spieler wÃ¤hlen",
    Options = GetPlayers(),
    Default = GetPlayers()[1],
    Callback = function(v) _G.TPTarget = v end
})

TPPlyr:AddButton({
    Name = "Zu Spieler teleportieren",
    ButtonText = "TP",
    Callback = function()
        local target = Players:FindFirstChild(_G.TPTarget or "")
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and Root() then
            Root().CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(3, 0, 0)
            Win:Notify({ Title = "Teleport", Message = "Zu " .. (_G.TPTarget or "?") .. " teleportiert!", Type = "Success", Duration = 3 })
        else
            Win:Notify({ Title = "Fehler", Message = "Spieler nicht gefunden!", Type = "Error", Duration = 3 })
        end
    end
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
--  TAB 4 - MISC
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
local TabMisc = Win:CreateTab("Misc", "âš™")

local Misc = TabMisc:CreateSection("Allgemein")

Misc:AddToggle({
    Name = "Anti AFK",
    Default = true,
    Callback = function(v) _G.AntiAFK = v end
})
local VU = game:GetService("VirtualUser")
LocalPlayer.Idled:Connect(function()
    if _G.AntiAFK then
        VU:Button2Down(Vector2.new(0, 0), CFrame.new())
        task.wait(1)
        VU:Button2Up(Vector2.new(0, 0), CFrame.new())
    end
end)

Misc:AddToggle({
    Name = "Hide GUI",
    Description = "Andere GUIs verstecken",
    Default = false,
    Callback = function(v)
        for _, g in pairs(LocalPlayer.PlayerGui:GetChildren()) do
            if g.Name ~= "SpeedLib" and g:IsA("ScreenGui") then
                g.Enabled = not v
            end
        end
    end
})

Misc:AddKeybind({
    Name = "Panic Key",
    Default = Enum.KeyCode.Delete,
    Callback = function()
        local sg = game:GetService("CoreGui"):FindFirstChild("SpeedLib")
        if sg then sg.Enabled = not sg.Enabled end
    end
})

Misc:AddButton({
    Name = "Sounds stoppen",
    ButtonText = "Stop",
    Callback = function()
        for _, s in pairs(workspace:GetDescendants()) do
            if s:IsA("Sound") then s:Stop() end
        end
        Win:Notify({ Title = "Sounds", Message = "Alle Sounds gestoppt.", Type = "Info", Duration = 2 })
    end
})

local Info = TabMisc:CreateSection("Info")
Info:AddLabel("âš¡ Speed Hub v2.0")
Info:AddLabel("Toggle: RightShift  |  Panic: Delete", Color3.fromRGB(255, 140, 0))
Info:AddSeparator()
Info:AddLabel("Made with SpeedLib ğŸ’›")

